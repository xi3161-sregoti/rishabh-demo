{
  "assume_role_arn": "arn:aws:iam::474532148129:role/TerraformAdmin",
  "cloudwatch_log_group_retention_in_days": 30,
  "cluster_name": "eks-eks",
  "cluster_version": "1.27",
  "common_tags": {
    "Environment": "Dev",
    "ManagedBy": "Terraform"
  },
  "control_plane_security_group_additional_rules": {
    "release_management_access": {
      "cidr_blocks": [
        "0.0.0.0/0"
      ],
      "description": "Release management access",
      "from_port": 443,
      "protocol": "tcp",
      "to_port": 443,
      "type": "ingress"
    }
  },
  "create_cni_ipv6_iam_policy": false,
  "eks_managed_node_group_defaults": {
    "ami_type": "AL2_x86_64",
    "create_launch_template": true,
    "disk_size": 50,
    "iam_role_attach_cni_policy": true,
    "security_group_rules": {
      "ingress_source_all": {
        "description": "Control plane to node all ports/protocols",
        "from_port": 0,
        "protocol": "-1",
        "source_cluster_security_group": true,
        "to_port": 0,
        "type": "ingress"
      }
    }
  },
  "eks_managed_node_groups": [
    {
      "capacity_type": "ON_DEMAND",
      "desired_size": 3,
      "instance_types": [
        "t3a.large"
      ],
      "labels": {
        "type": "memory-intensive"
      },
      "max_size": 9,
      "min_size": 3,
      "name": "worker-1",
      "taints": {}
    }
  ],
  "enable_endpoint_private_access": true,
  "enable_endpoint_public_access": true,
  "enable_irsa": true,
  "enabled_cluster_log_types": [
    "audit",
    "api",
    "authenticator"
  ],
  "ip_family": "ipv4",
  "kms_key_arn": "arn:aws:kms:ap-south-1:474532148129:key/f94f1efc-0b6c-4959-87ac-bc1dde13e504",
  "node_security_group_additional_rules": {
    "ingress_allow_access_from_control_plane_opa": {
      "description": "Allow access from control plane to webhook port of OPA Gatekeeper",
      "from_port": 0,
      "protocol": "-1",
      "source_cluster_security_group": true,
      "to_port": 0,
      "type": "ingress"
    }
  },
  "public_access_cidrs": [
    "0.0.0.0/0"
  ],
  "region": "ap-south-1",
  "subnet_ids": [
    "subnet-0b0f62f5c9a6697d4",
    "subnet-0afccdde86ef8e672",
    "subnet-09a2db7fdac92e25c"
  ],
  "tags": {
    "project": "Xlr8s"
  },
  "vpc_id": "vpc-054d1a3db9b2d4c05"
